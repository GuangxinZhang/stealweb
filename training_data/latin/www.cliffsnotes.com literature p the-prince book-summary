<script type="text/javascript">
    function getShareLink() {
        var litnote_link = document.getElementById('litnote-link');
        if (litnote_link) {
            var share_link = {
                url: (litnote_link.href).replace(/^\//, 'http://' + document.domain + '/'),
                title: litnote_link.title + ' - Cliffsnotes'
            }
            return share_link;
        }
        return false;
    };
</script>






