<script type="text/javascript">
    function Om_Search(nosearchkeyword, searchclickthroughlink, searchkeyword) {
        if (nosearchkeyword != undefined && nosearchkeyword != "") dl.nosearchkeyword = nosearchkeyword;
        if (searchclickthroughlink != undefined && searchclickthroughlink != "") dl.searchclickthroughlink = searchclickthroughlink;
        if (searchkeyword != undefined && searchkeyword != "") dl.searchkeyword = searchkeyword;
    }

    var referrer = document.referrer;
    var currentPage = document.location.href;
    if (referrer != undefined && referrer.indexOf('/search?') != -1 && currentPage != undefined && currentPage.indexOf('/search?') == -1) {
        if (typeof Om_Search == "function") {
            Om_Search("", currentPage, "");
        }
    }
</script>
