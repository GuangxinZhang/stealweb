        		<script>
        		if (!!window._geo) (function(e){
        			if (!e) return console.error('Cannot find element');

        			var privateMode = _geo.isPrivateMode;

        			var f = $('#location-picker');
        			$(e).find('.target-locale').on('click', function(evt){
        				evt.stopPropagation();
        				evt.preventDefault();
        				f.toggleClass('visible');
        				return false;
        			});
        			f.on('click', '.option', function(evt){
        				var d = $(this).data('locale-d'),
        					s = $(this).data('locale-s'),
        					v = new Date().setTime(new Date().getTime() + 3600 * 24 * 7 * 1000),
        					u = window.location.host.indexOf('.dv.') !== -1?
        						window.location.host.replace('.dv.', '.apple.') : window.location.host.replace('.apple.', '.dv.');

        				// return console.log(`D ${d}, S ${s}`);

        				
        				window._geo.storage.setItem('localeD', d);
        				window._geo.storage.setItem('localeS', s);
        				window._geo.storage.setItem('localeCC', 'US');
        				window._geo.storage.setItem('localeValid', v);
        				
        				f.removeClass('visible');
        				
        				var hash = encodeURIComponent( JSON.stringify({
        					'localeCC' : 'US',
        					'localeS' : s,
        					'localeD' : d,
        					'localeValid' : v,
        					'url' : 'http://hk.apple.nextmedia.com'
        				}) );

        				$('<iframe style="display:none" src="http://' + u + '/pass_geo?v12#' + hash + '">').appendTo($('body'));
        			});

        			$('body').get(0).addEventListener('click', function(e){
        				f.removeClass('visible');
        				return true;
        			}, false);

        			// copy the locale setting...
        			(function(){

        				var d = _geo.storage.getItem('localeD'),
        					s = _geo.storage.getItem('localeS'),
        					cc = _geo.storage.getItem('localeCC'),
        					v = _geo.storage.getItem('localeValid'),
        					u = window.location.host.indexOf('.dv.') !== -1?
        						window.location.host.replace('.dv.', '.apple.') : window.location.host.replace('.apple.', '.dv.');

        					var hash = encodeURIComponent( JSON.stringify({
	        					'localeCC' : cc,
	        					'localeS' : s,
	        					'localeD' : d,
	        					'localeValid' : v	        					
	        				}) );

        				$('<iframe style="display:none" src="http://' + u + '/pass_geo?v12#' + hash + '">').appendTo($('body'));        				
        			})();

        		})(document.getElementById('location-select'));
        		</script>
