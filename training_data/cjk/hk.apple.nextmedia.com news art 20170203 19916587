		run : function(){
			var _self = this;

			// Good IE doesn't even have String.trim()...
			if (typeof String.prototype.trim !== 'function') {
				String.prototype.trim = function() {
					return this.replace(/^\s+|\s+$/g, ''); 
				}
			}

			// Articles introduction and inner text	
			var pattern = '攝影(：|︰)(';
			for (i in _self.Mapping){
				pattern += i + '|';								
			}
			pattern = pattern.substr(0, pattern.length-1) + ')';
			// pattern += "、|　)";
			var rPattern = new RegExp(pattern, "ig");
			var el = $('.zoom_image_caption div').add($('a.fancybox-button span')).add($('p.ArticleIntro')).add($('.ArticleContent_Inner p')).add($('.fancybox-button'));

			var excludePattern = '(';
			for (i in _self.Mapping){
				excludePattern += i + '|';								
			}			
			excludePattern = excludePattern.substr(0, excludePattern.length-1) + ')';
			var xPattern = new RegExp(excludePattern, 'ig');
			el.find('img').each(function(){
				var me = $(this);
				if (typeof me.attr('title') !== 'undefined'){
					var xTitle = me.attr('title').replace(xPattern, "________$1________");
					me.attr('title', xTitle);	
				}
				// var xTitle = me.attr('title').replace(xPattern, "________$1________");
				// me.attr('title', xTitle);
			});

			// $('p.ArticleIntro').add($('.ArticleContent_Inner p')).each(function(){
			// $('.zoom_image_caption div').add($('a.fancybox-button span')).add($('p.ArticleIntro')).add($('.ArticleContent_Inner p')).add($('.fancybox-button')).each(function(){		
			el.each(function(){
				var src = $(this).html(), html = '攝影︰';
				// src = src.replace(/\s+/ig, ' ');
				// src = src.replace(/攝影(：|︰)(.*?)/ig, function(match, colon, names){
				src = src.replace(rPattern, function(match, colon, names){
					var photographers = names.split(/[、　]/);
					// var photographers = names.split('、');
					// if (photographers.length == 1) photographers = names.split('　');

					for (x = 0, y = photographers.length; x < y; x++){
						var p = photographers[x].trim();						
						var obj = _self.Mapping[p];
						if (typeof(obj) !== 'undefined') {
							var link = obj.link, photo = obj.photo;
							html += '<a class="photographer-button" href="' + link + '" target="_photographer"><img src="' + photo + '" width="18" height="18" />' + photographers[x] + '</a>';
						} else html += photographers[x].trim();
						if (x != y - 1) html += '、';
					}
					return html;					
				});					
				$(this).html(src);					
			});

			// Image caption
			var pattern = '(';
			for (i in _self.Mapping){
				pattern += i + '|';								
			}
			pattern = pattern.substr(0, pattern.length-1) + ')攝';
			var rPattern = new RegExp(pattern, "ig");
			// $('.zoom_image_caption div').add($('a.fancybox-button span')).each(function(){
			// $('.zoom_image_caption div').add($('a.fancybox-button span')).add($('p.ArticleIntro')).add($('.ArticleContent_Inner p')).add($('.fancybox-button')).each(function(){
			el.each(function(){
				var src = $(this).html();
				src = src.replace(rPattern, function(match, name){						
					var obj = _self.Mapping[name];						
					if (typeof(obj) !== 'undefined') {
						var link = obj.link, photo = obj.photo;
						return name + '攝 <a class="" href="' + link + '" target="_photographer"><img src="http://staticlayout.apple.nextmedia.com/web_images/layout/photojouralist%20icon_18x18.jpg" width="18" height="18" /></a>';
					} else return name;
				});					
				$(this).html(src);					
			});

			el.find('img').each(function(){
				var me = $(this);
				var reversePatternString = '________' + excludePattern + '________';
				var reversePattern = new RegExp(reversePatternString, 'ig');
				if (typeof me.attr('title') !== 'undefined'){					
					var xTitle = me.attr('title').replace(reversePattern, "$1");					
					me.attr('title', xTitle);	
				}				
			});

			// Fancy Box titles						
			$('.fancybox-button').each(function(){				
			// $('.zoom_image_caption div').add($('a.fancybox-button span')).add($('p.ArticleIntro')).add($('.ArticleContent_Inner p')).add($('.fancybox-button')).each(function(){	
				var src = $(this).attr('title');
				src = src.replace(rPattern, function(match, name){						
					var obj = _self.Mapping[name];						
					if (typeof(obj) !== 'undefined') {
						var link = obj.link, photo = obj.photo;
						return name + '攝 <a class="" href="' + link + '" target="_photographer"><img src="http://staticlayout.apple.nextmedia.com/web_images/layout/photojouralist%20icon_18x18.jpg" width="18" height="18" /></a>';
					} else return name;
				});					
				$(this).data("title-processed", src);
				$(this).on("click", "a", function(e){					
					e.stopPropagation();					
					return true;
				});				
			});

		}
	}
	Stylizer.run();
	// console.info(Stylizer.Mapping);
});
</script>


	
