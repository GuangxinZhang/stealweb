      /* Generate background position for Zoom Fade effect.*/
      function wds_genBgPos_0(current_key) {
        var bgSizeArray = [0, 70];
        var bgSize = bgSizeArray[Math.floor(Math.random() * bgSizeArray.length)];
        var bgPosXArray = ['left', 'right'];
        var bgPosYArray = ['top', 'bottom'];
        var bgPosX = bgPosXArray[Math.floor(Math.random() * bgPosXArray.length)];
        var bgPosY = bgPosYArray[Math.floor(Math.random() * bgPosYArray.length)];
        jQuery(current_key + " .wds_slideshow_image_0").css({
          backgroundPosition: bgPosX + " " + bgPosY,
          backgroundSize : (100 + bgSize) + "% " + (100 + bgSize) + "%",
          webkitAnimation: ' wdszoom' + bgSize + ' 4s linear 0s alternate infinite',
          mozAnimation: ' wdszoom' + bgSize + ' 4s linear 0s alternate infinite',
          animation: ' wdszoom' + bgSize + ' 4s linear 0s alternate infinite'
        });
      }
