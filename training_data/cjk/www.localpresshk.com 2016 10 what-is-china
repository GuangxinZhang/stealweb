                });
              }
            }
            if (now >= 270) {
              if (now < 361) {
                top_left_deggree_0  = now - 270;
                jQuery('#top_left_0').css({
                  '-moz-transform':'rotate('+top_left_deggree_0 +'deg)',
                  '-webkit-transform':'rotate('+top_left_deggree_0 +'deg)',
