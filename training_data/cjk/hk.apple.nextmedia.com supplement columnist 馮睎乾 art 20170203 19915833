	function do_search(keyword)
	{
		String.prototype.trim=function(){return this.replace(/(^\s*)|(\s*$)/g,"");}
		keyword = keyword.toString();
		keyword = keyword.replace(/[`~!@#$%^&*()_|+\-=?;:'",.<>\{\}\[\]\\\/]/gi, '');
		keyword = keyword.trim();
		if(keyword == "" || keyword == "搜尋")
		{
			alert("請輸入搜尋關鍵字");
			return;
		}
		var searchForm=document.getElementById("searchForm");
		  searchForm.action = "/search/" + encodeURIComponent(keyword)+"?q="+encodeURIComponent(keyword);
		  searchForm.submit();	
	}
	
	function do_tag_search(keyword)
	{
		String.prototype.trim=function(){return this.replace(/(^\s*)|(\s*$)/g,"");}
		keyword = keyword.toString();
		keyword = keyword.trim();
		if(keyword == "" || keyword == "搜尋")
		{
			alert("請輸入搜尋關鍵字");
			return;
		}
		var searchForm=document.getElementById("searchForm");
		  searchForm.action = "/tagsearch/" + encodeURIComponent(keyword)+"?q="+encodeURIComponent(keyword);
		  searchForm.submit();	
	}
	
	function do_stock(keyword)
	{
		String.prototype.trim=function(){return this.replace(/(^\s*)|(\s*$)/g,"");}
		keyword = keyword.toString();
		keyword = keyword.trim();
		if(keyword == "" || isNaN(keyword))
		{
			alert("請輸入有效數字");
			return;
		}
		Krux('admEvent','JhMh5r6G', {});
		window.open('http://quote.appledaily.com/?quote='+parseInt(keyword, 10));
	}	
	
	function isEnter(event , keyword)
	{
		 if(event.keyCode == 13)
		 {
		 	do_search(keyword);
		 }
	}	
	
	function isEnterStock(event , keyword)
	{
		 if(event.keyCode == 13)
		 {
		 	do_stock(keyword);
		 }
	}	
	
	
	function fbSharer(url)
	{
		//var shareurl = encodeURI("http://www.facebook.com/sharer/sharer.php?u="+url);
		var shareurl = "http://www.facebook.com/sharer/sharer.php?u="+encodeURIComponent(url);
	 	window.open(shareurl,'','width=550,height=350,menubar=no,status=no');
	}
	

	function twitterSharer(url , title)
	{
		var queryStr = 'ref%3Dtw';	//ref=tw
		if (url.indexOf('?') !== -1)
			queryStr = '%26' + queryStr;	// &ref=tw
		else
			queryStr = '%3F' + queryStr;	// ?ref=tw
		var shareurl = "http://twitter.com/share?url="+encodeURIComponent(url)+queryStr;
		var sharetitle = encodeURI(title);
		window.open(shareurl+'&text='+sharetitle+'%20+%23%E8%98%8B%E6%9E%9C%E6%97%A5%E5%A0%B1+','','width=550,height=300,menubar=no,status=no');
	}	
	

</script>	
