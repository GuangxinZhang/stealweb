								<script>            		
        			(function(id, groups){

        				var d = window._geo && _geo.storage.getItem('localeD'),
        					cc = (window._geo && _geo.storage.getItem('localeCC')) || 'HK',
        					printed = false;

        				for (var k in groups) {
        					
        					var g = groups[k];
        					
        					if (k === '焦點新聞' && cc !== 'HK') continue;
        					if (k.indexOf('焦點新聞,') !== -1 && cc === 'HK') continue;

        					if (k === '焦點新聞,us') {
        						if (cc !== 'US') continue;
        						if ([501, 803, 807].indexOf(+d) !== -1) continue;
        					}

        					if (k === '焦點新聞,ussf' && d !== '807') continue;
        					if (k === '焦點新聞,usny' && d !== '501') continue;
        					if (k === '焦點新聞,usla' && d !== '803') continue;
        					if (k === '三藩市' && d !== '807') continue;
        					if (k === '紐約' && d !== '501') continue;
        					if (k === '洛杉磯' && d !== '803') continue;
        					if (k === '美國' && cc !== 'US') continue;

        					if (printed) continue;

        					for (var i = 0, j = g.length; i < j; i ++) {

        						var article = g[i];

        						if (+(article.id) === +id && article.article_url.indexOf(window.location.pathname) !== -1){
        							printed = true;

        							// has prev
        							if (i > 0) {
        								var prev = g[i-1];

        								// prev.title = prev.title.replace(/\s\((HKT)?.*\)$/, '');
        								prev.title = prev.title.replace(/\s\((HKT\s)?\d{1,2}:\d{1,2}(am|pm)\)$/, '');

										document.write('<div class="prevBox mouseOver" onclick="javascript: window.location=\'' + prev.article_url + '\'">');

										if (!!prev.image && prev.image !== ''){
											document.write('<div class="photo">');
											document.write('<img src="' + prev.image +  '"/>');
											document.write('</div>');											
										}

										document.write('<div class="nav">上一則</div>');
										document.write('<div class="title">' + prev.title + '</div>');
										document.write('<div class="clear"></div>');
										document.write('</div>');

        							}

        							if (i < j - 1) {
        								var next = g[i+1];

        								next.title = next.title.replace(/\s\((HKT\s)?\d{1,2}:\d{1,2}(am|pm)\)$/, '');

        								document.write('<div class="nextBox mouseOver" onclick="javascript: window.location=\'' + next.article_url + '\'">');

										if (!!next.image && next.image !== ''){
											document.write('<div class="photo">');
											document.write('<img src="' + next.image +  '"/>');
											document.write('</div>');											
										}

										document.write('<div class="nav">下一則</div>');
										document.write('<div class="title">' + next.title + '</div>');
										document.write('<div class="clear"></div>');
										document.write('</div>');
        							}

        						}

        					}        					

        				}
        				
