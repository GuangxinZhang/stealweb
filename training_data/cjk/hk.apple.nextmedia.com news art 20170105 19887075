<script>
function calculateRTLayout(){	
	
	if ($('.page_realtime_main_title').size()) (function(){
		var inner = $('.LHSTitle_inner_big');					

		if ($('.page_realtime_main_title li:visible:first').position().top !== $('.page_realtime_main_title li:visible:last').position().top){
			$('<div class="left-row"></div>').prependTo(inner);
			$('<div class="right-row"></div>').prependTo(inner);
			inner.addClass('row-2');
		} else inner.removeClass('row-2');

	})();
}
calculateRTLayout();
</script>
