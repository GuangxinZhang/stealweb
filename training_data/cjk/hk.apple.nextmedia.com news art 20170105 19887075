				function changeStyle(num) {
					cur = getCookie("fontsize")!=""?getCookie("fontsize"):2;
					//document.getElementById("masterContent").className = "fontSize" + num;
					$("#masterContent").removeClass("fontSize2")
					$("#masterContent").removeClass("fontSize3");
					$("#masterContent").addClass("fontSize" + num);
					setCookie("fontsize",num,1000);
					try{
						document.getElementById("selectFontsize" + cur).className = 'fontsize' + cur;
						document.getElementById("selectFontsize" + num).className = 'fontsize' + num + 'on';
					}catch(e){}
				}
				
				function setCookie(name,value,days_expires) {
					var exp = new Date();
					exp.setTime(exp.getTime() + (days_expires*60*60));
					document.cookie = name + "=" + escape(value) + ";path=/" + ((days_expires == 0)? " " : "; expires=" + exp.toGMTString());
				}
				function getCookie(name) {
					var rtn = "";
					if (document.cookie.match(new RegExp(name + "\\=([^\\;])[\\;]{0,1}", "gi")))
						rtn = unescape(RegExp.$1);
						
					return rtn;
				}	
				
				
