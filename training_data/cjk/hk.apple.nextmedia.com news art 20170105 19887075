var lang='TC';
var targetingTag = {
	ksg : Krux.segments,
	kuid : Krux.user,
	article : '19887075' || undefined,
	subsectname : 'others' || undefined
};

if (targetingTag.article === undefined) {
	delete targetingTag.article;
	delete targetingTag.subsectname;
}

function getCookie(cname) {

    var name = cname + "=";
    var ca = document.cookie.split(';');
   	for(var i = 0; i <ca.length; i++) {
        var c = ca[i];
		while (c.charAt(0)==' ') {
			c = c.substring(1);
		}
		if (c.indexOf(name) == 0) {
        	return c.substring(name.length,c.length); 	
		}
	}
	return ""; 	
}

if (getCookie("googtrans") === '/zh-CN/zh-CN') lang='SC';

if (!!window._geo) {	
	targetingTag.D = _geo.storage.getItem('localeD');
	targetingTag.CC = _geo.storage.getItem('localeCC');
	targetingTag.S = _geo.storage.getItem('localeS');
	targetingTag.L = lang;
}
