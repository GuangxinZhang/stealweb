<script type="text/javascript">
	var $window = $(window);
   	var doorLeft = $('#divSkyscraper1');
    var doorRight = $('#divSkyscraper2');
    var documentHeight = $(document).height();
    var topDefaultMargin = 137;
    $(window).scroll(function() {
    	var scrollTop = $window.scrollTop();
    	var leftTopPos = doorLeft.offset().top;
    	var rightTopPos = doorRight.offset().top;
    	
		if(scrollTop > topDefaultMargin) {
    		var topValue = scrollTop - topDefaultMargin + 10;
    		doorLeft.css('top', topValue+'px');
    		doorRight.css('top', topValue+'px');
    	} else{
    		doorLeft.css('top', '10px');
    		doorRight.css('top', '10px');
    	}
   	}).trigger('scroll');
</script>



