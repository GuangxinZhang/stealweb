        var hideAd = JSON.parse(window.sessionStorage.getItem('hideClosableAd'));

        if (hideAd) return;

        adBtn
        .text('關閉廣告')
        .removeClass('hidden')
        .click(function() {
          slotTagElement.remove();
          slotTagElement.hide(function() {
            window.sessionStorage.setItem('hideClosableAd', true);
          });
        });
      });
    })
  </script>

  
  

  
  
