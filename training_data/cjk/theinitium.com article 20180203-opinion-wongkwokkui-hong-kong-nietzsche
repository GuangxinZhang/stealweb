  <script>
    googletag.cmd.push(function() {
      googletag.pubads().addEventListener('slotRenderEnded', function(event) {
        if (event.slot.getSlotElementId() !== 'div-gpt-ad-123456788-8' || event.isEmpty) return;

        var $prestitialAd = $('.c-ad-prestitial');
        var $body = $('body');

        // toggle ad
        $prestitialAd.fadeIn(function() {
          $body.addClass('u-noScroll');
        });
        $(window).click(function() {
          $prestitialAd.fadeOut(function() {
            $body.removeClass('u-noScroll');
          });

          closeAd();
          $(window).off('click');
        });

        /**
         * mobile phone only
         */
        if ($(window).width() >= 620) return;

        // resize
        $('#div-gpt-ad-123456788-8 > div > iframe').css({
          position: 'absolute',
          left: 0,
          top: 0,
          width: '100%',
          height: '100%',
        });
        // close button
        $prestitialAd.find('.close-btn').removeClass('hidden');
      });
    });
  </script>
  
