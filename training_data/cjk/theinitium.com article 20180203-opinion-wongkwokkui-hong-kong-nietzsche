  <script>
    $.endlessPaginate({
      paginateOnScroll: true,
      paginateOnScrollMargin: 20
    });

    LinkTransformer.init('a[data-track-article-link-top]', 'Article_Links', 'Click_Top_Section');
    LinkTransformer.init('a[data-track-article-link-editorchoice]', 'Article_Links', 'Click_EditorChoice_Section');
    LinkTransformer.init('a[data-track-further-reading-item]', 'Article_Links', 'Click_FurtherReading_Section');

    var commentTextareaElem = $('[data-comment-textarea]');
    var wordCountElem = $('[data-comment-words-current]');
    var submitButtonElem = $('[data-comment-submit]');
    var commentFormElem = $('[data-comment-form]');

    var submitted = false;

    submitButtonElem.on('click', function (event) {
      event.preventDefault();
      if (submitted || submitButtonElem.prop('disabled')) return;
      submitted = true;
      submitButtonElem.prop('disabled', true);

      commentFormElem.submit();
      return false;
    });

    function validateCommentContent(content) {
      if (submitted) return;
      var len = content.length;
      wordCountElem.html(len);
      if (len > 3000 || len == 0) {
        wordCountElem.css('color', 'red');
        submitButtonElem.prop('disabled', true);
      } else {
        wordCountElem.css('color', '');
        submitButtonElem.prop('disabled', false);
      }
    }

    // display comment actions after the textarea is on focus
    commentTextareaElem
    .focus(function (event) {
      validateCommentContent(event.target.value);
    })
    .on('change keyup paste', function (event) {
      validateCommentContent(event.target.value);
    });
  </script>




