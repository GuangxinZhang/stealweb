  <button class="ad-btn" style="" type="button">隱藏廣告Ｘ</button>
  <script type="text/javascript">
    googletag.cmd.push(function() {
      googletag.pubads().addEventListener('slotRenderEnded', function(event) {
        if (event.slot.getSlotElementId() !== 'div-gpt-ad-123456788-0') return;
        if (event.isEmpty) return;

        var HIDE_AD_TEXT = '隱藏廣告Ｘ';
        var SHOW_AD_TEXT = '顯示廣告▲';
        var slotTagElement = $('#div-gpt-ad-123456788-0');
        var adBtn = slotTagElement.parent().find('.ad-btn');
        var hideAd = JSON.parse(window.sessionStorage.getItem('hiddenClosableAd'));

        if (hideAd) {
          slotTagElement.hide();
          adBtn.text(SHOW_AD_TEXT);
        } else {
          adBtn.text(HIDE_AD_TEXT)
        }

        adBtn.show()
        .click(function() {
          slotTagElement.slideToggle(700, function() {
            var hidden = slotTagElement.css('display') === 'none';
            // Toggle Ad
            adBtn.text(hidden ? SHOW_AD_TEXT: HIDE_AD_TEXT);
            // Hide Ad until browser is closed
            window.sessionStorage.setItem('hiddenClosableAd', JSON.stringify(hidden));
          });
        });
      });
    })
  </script>
  

