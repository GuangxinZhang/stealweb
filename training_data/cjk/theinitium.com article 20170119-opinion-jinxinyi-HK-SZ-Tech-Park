  <script src="https://d32kak7w9u5ewj.cloudfront.net/static/js/jquery.modal.min.js"></script>
  <script>
    // override the default model class `modal` since it conflicts with existing css styles
    $.modal.defaults.modalClass = 'jq-modal';
    $('a[data-modal]').click(function(event) {
      $(this).modal();
      return false;
    });

    $.endlessPaginate({
      paginateOnScroll: true,
      paginateOnScrollMargin: 20
    });

    LinkTransformer.init('a.js-article-link-top', 'Article_Links', 'Click_Top_Section');
    LinkTransformer.init('a.js-article-link-editorchoice', 'Article_Links', 'Click_EditorChoice_Section');
    LinkTransformer.init('a.js-further-reading-item', 'Article_Links', 'Click_FurtherReading_Section');

    var commentTextareaElem = $('.js-comment-textarea');
    var wordCountElem = $('#js-comment-words-current');
    var submitButtonElem = $('#js-comment-submit');
    var commentFormElem = $('.js-comment-form');

    var submitted = false;

    submitButtonElem.on('click', function (event) {
      event.preventDefault();
      if (submitted || submitButtonElem.prop('disabled')) return;
      submitted = true;
      submitButtonElem.prop('disabled', true);
      commentFormElem.submit();
      return false;
    });

    function validateCommentContent(content) {
      if (submitted) return;
      var len = content.length;
      wordCountElem.html(len);
      if (len > 3000 || len == 0) {
        wordCountElem.css('color', 'red');
        submitButtonElem.prop('disabled', true);
      } else {
        wordCountElem.css('color', '');
        submitButtonElem.prop('disabled', false);
      }
    }
    // display comment actions after the textarea is on focus
    commentTextareaElem.focus(function (event) {
      validateCommentContent(event.target.value);
      $('.comment-actions').css('display', 'block');
      commentTextareaElem.attr('rows', 5);
      commentTextareaElem.attr('placeholder', '');
    });

    commentTextareaElem.on('change keyup paste', function (event) {
      validateCommentContent(event.target.value);
    });

    $('.js-comment-login').click(function (event) {
      event.stopPropagation();
      event.preventDefault();
      window.location = '/auth/login/?next=' + window.location.pathname + '#comments';
      return false;
    });
    $('.js-comment-nickname').click(function (event) {
      event.stopPropagation();
      event.preventDefault();
      window.location = '/auth/nickname/?next=' + window.location.pathname + '#comments';
      return false;
    })
  </script>
  <script>
    $(function() {
      var $window = $(window);

      if ($(window).width() <= 980) return;

      var furtherReadingElem = $('.js-further-reading');
      var sideContent = $('.side-content');

      furtherReadingElem.css({
        width: sideContent.width(),
        marginTop: 0,
      })
      .find('.title-text').css({
        position: 'relative',
        top: 4,
        background: '#f2f2f2',
        lineHeight: 1,
      }).end();

      // auto adjust height
      var contentElem = furtherReadingElem.find('.further-readings-content');
      var footerHeight = $('.global-nav-footer').css('height');
      var footerMarinTop = $('.global-nav-footer').css('marginTop');

      contentElem.css({
        maxHeight: ('calc(100vh - ' + footerHeight + ' - ' + footerMarinTop + ' - (15px + 19px + 40px))'),
        overflow: 'scroll',
      });
      sideContent.append(furtherReadingElem);

      // Sticky @ Top
      var prevSideElem = furtherReadingElem.prev();
      $window.scroll(function() {
        if ($('.main-content').height() < $('.side-content').height()) return;

        var sideAdOffsetTop = prevSideElem.offset().top + prevSideElem.height();
        var windowScrollTop = $window.scrollTop();

        if (windowScrollTop > sideAdOffsetTop) {
          furtherReadingElem.addClass('fixed-block-item');
        } else {
          furtherReadingElem.removeClass('fixed-block-item');
        }
      });
    });
  </script>




