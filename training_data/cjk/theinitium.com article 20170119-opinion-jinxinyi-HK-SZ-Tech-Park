<script>
  $(function() {
    var ua = window.navigator.userAgent;
    var $deepLinkElem = $('.js-deep-link');
    var deepLink;

    if (ua.match(/(iPhone|iPod|iPad)/i)) {
      deepLink = $deepLinkElem.data('ios-link');
    } else if (ua.match(/Android/i)) {
      deepLink = $deepLinkElem.data('android-link');
    } else {
      $deepLinkElem.closest('.mobile_app_banner').hide();
    }

    deepLink && $deepLinkElem.attr('href', deepLink);
  });
</script>

  
  
  <script src="https://d32kak7w9u5ewj.cloudfront.net/static/js/echarts.min.js"></script>
  <script src="https://d32kak7w9u5ewj.cloudfront.net/static/js/echarts.theme.js"></script>

  <script>
  $('.widget-echart .echarts').each(function (idx) {
    var elem = $(this);
    if ($('.main-content').width() < 620) {
      return;
    }

    elem.show();
    var chartAssetUuid = elem.attr('data-asset-uuid');

    if (!chartAssetUuid) {
      return;
    }

    var chart = echarts.init(this, 'initium');
    chart.showLoading();
    // hide the image
    elem.parent().find('img').css('display', 'none');

    var chartDataUrl = '/asset/echart/' + chartAssetUuid;
    
    var drawChart = function() {
      $.get(chartDataUrl).done(function(data) {
        chart.hideLoading();
        chart.setOption(data);
      });
    };

    if (elem.is('[data-geojson]')) {
      geojson_url = chartDataUrl + '?geojson';
      
      $.get(geojson_url).done(function(geoJSON) {
        echarts.registerMap('GeoJSON', geoJSON);
        drawChart();
      });
    } else {
      drawChart();
    }
  });
  </script>

