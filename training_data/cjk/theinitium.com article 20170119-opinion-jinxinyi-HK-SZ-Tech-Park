  <script>
    googletag.cmd.push(function() {
      googletag.pubads().addEventListener('slotRenderEnded', function(event) {
        if (event.slot.getSlotElementId() !== 'div-gpt-ad-123456788-8' || event.isEmpty) return;

        var isMobile = navigator.userAgent.match(/(iPhone|iPod|iPad|Android|BlackBerry)/i);
        var $prestitialAd = $('.ad-prestitial');
        var $adWrapper = $prestitialAd.find('.ad-wrapper');
        var $html = $('html');

        // close btn
        if (isMobile) {
          $prestitialAd.append('<button class="ad-btn ad-btn--close" type="button"><i class="icon icon-close"></i></button>');
        } else {
          $adWrapper.append('<button class="ad-btn" type="button">關閉廣告Ｘ</button>');
        }

        // toggle ad
        $prestitialAd.fadeIn(function() {
          $html.css({ 'overflow': 'hidden' });
        });
        $(window).click(function() {
          $prestitialAd.fadeOut(function() {
            $html.removeAttr('style');
          });
          $(window).off('click');
        });

        // mobile phone
        if ($(window).width() >= 620) return;
        $adWrapper.css({
          width: '100%',
          height: '100%',
          margin: '0 10px',
        });
        $('#div-gpt-ad-123456788-8 > div > iframe').css({
          position: 'absolute',
          left: 0,
          top: 0,
          width: '100%',
          height: '100%',
        });
      });
    });
  </script>
  
</div>






